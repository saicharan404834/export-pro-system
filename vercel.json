{
  "version": 2,
  "builds": [
    {
      "src": "bls-exportpro/frontend/package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist"
      }
    },
    {
      "src": "bls-exportpro/backend/vercel.ts",
      "use": "@vercel/node",
      "config": {
        "includeFiles": [
          "bls-exportpro/shared/**",
          "bls-exportpro/backend/data/**",
          "bls-exportpro/backend/src/**",
          "bls-exportpro/backend/dist/**",
          "bls-exportpro/backend/uploads/**",
          "bls-exportpro/backend/package.json"
        ]
      }
    }
  ],
  "routes": [
    {
      "src": "/health",
      "dest": "bls-exportpro/backend/vercel.ts"
    },
    {
      "src": "/api/(.*)",
      "dest": "bls-exportpro/backend/vercel.ts"
    },
    {
      "src": "/uploads/(.*)",
      "dest": "bls-exportpro/backend/vercel.ts"
    },
    {
      "src": "/(.*\\.(js|css|png|jpg|jpeg|gif|svg|ico|woff|woff2|ttf|eot|map|json))",
      "dest": "bls-exportpro/frontend/dist/$1",
      "headers": {
        "Cache-Control": "public, max-age=31536000, immutable"
      }
    },
    {
      "src": "/(.*)",
      "dest": "bls-exportpro/frontend/dist/index.html"
    }
  ],
  "env": {
    "NODE_ENV": "production"
  },
  "functions": {
    "bls-exportpro/backend/vercel.ts": {
      "maxDuration": 30
    }
  }
}
