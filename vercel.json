{
  "version": 2,
  "functions": {
    "api/index.ts": {
      "maxDuration": 30
    }
  },
  "routes": [
    {
      "src": "/health",
      "dest": "/api"
    },
    {
      "src": "/api/(.*)",
      "dest": "/api"
    },
    {
      "src": "/uploads/(.*)",
      "dest": "/api"
    },
    {
      "src": "/(.*)",
      "dest": "/$1"
    }
  ],
  "env": {
    "NODE_ENV": "production"
  }
}
